<div class="subnav subnav-fixed nav-collapse">
    {% for module_group_name, modules in menu %}
        <ul class="nav nav-pills">
            {% if module.route is defined %}
                <li>
                    <a href="{{ path( module.route, module.params|default([])) }}">
                        {% if module.icon is defined %}
                            <i class="{{ module.icon }}"></i>
                        {% endif %}{{ module_name }}
                    </a>
                </li>
            {% else %}
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        Foo
                    </a>
                    <ul class="dropdown-menu">
                        <li>Bar</li>
                        <li>Baz</li>
                    </ul>
                </li>
            {% endif %}
        </ul>
    {% endfor %}
</div>
